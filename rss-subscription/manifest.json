{
   "background_page": "background.html",
   "content_scripts": [ {
      "js": [ "feed_finder.js" ],
      "matches": [ "http://*/*", "https://*/*" ]
   } ],
   "description": "Adds one-click subscription to your toolbar. Adds support for the registered feed handler, such as Outlook 2007 or 2010.",
   "icons": {
      "128": "feed-icon-128x128.png"
   },
   "name": "RSS Subscriptions with FEED: Handler Support",
   "page_action": {
      "default_icon": "feed-icon-16x16.png",
      "default_title": "Subscribe to this feed",
      "popup": "popup.html"
   },
   "permissions": [ "tabs", "http://*/*", "https://*/*" ],
   "version": "1.8.5"
}
